{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "(Windows) build",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/MDd",
                "/nologo",
                "/std:c++20",
                "/I${workspaceFolder}\\third_party",
                "/I${workspaceFolder}\\include",
                "/Fe${workspaceFolder}\\bin\\main.exe",
                "${workspaceFolder}\\main.cpp",
                "${workspaceFolder}\\include\\database\\mysql.cpp",
                "${workspaceFolder}\\third_party\\mysql\\lib\\libmysql.lib",
            ],
            "options": {
                "cwd": "${workspaceFolder}\\bin"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}